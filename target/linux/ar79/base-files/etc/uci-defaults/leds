#!/bin/sh
#
# Copyright (C) 2011 OpenWrt.org
#

. /lib/functions/uci-defaults.sh
. /lib/ar71xx.sh

board=$(ar71xx_board_name)

case "$board" in
rl-ans5004 ) 
	ucidef_set_led_timer  "system" "system" "richerlink:red:system" "50000" "1"
	;;
rl-ans5001 ) 
	ucidef_set_led_netdev  "wlan" "WLAN" "rl_ans5001:green:wlan" "wifi0"
	ucidef_set_led_netdev "wan" "WAN"  "rl_ans5001:green:wan" "eth0"
	ucidef_set_led_timer  "system" "system" "rl_ans5001:red:system" "50000" "1"
	;;
esac

ucidef_commit_leds

exit 0
