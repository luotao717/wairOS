obj-y	:= prom.o setup.o irq.o common.o clock.o gpio.o

obj-$(CONFIG_EARLY_PRINTK)		+= early_printk.o
obj-$(CONFIG_PCI)			+= pci.o

#
# Devices
#
obj-y					+= dev-common.o
obj-$(CONFIG_AR79_DEV_AP9X_PCI)	+= dev-ap9x-pci.o
obj-$(CONFIG_AR79_DEV_DSA)		+= dev-dsa.o
obj-$(CONFIG_AR79_DEV_ETH)		+= dev-eth.o
obj-$(CONFIG_AR79_DEV_GPIO_BUTTONS)	+= dev-gpio-buttons.o
obj-$(CONFIG_AR79_DEV_LEDS_GPIO)	+= dev-leds-gpio.o
obj-$(CONFIG_AR79_DEV_M25P80)		+= dev-m25p80.o
obj-$(CONFIG_AR79_DEV_NFC)		+= dev-nfc.o
obj-$(CONFIG_AR79_DEV_SPI)		+= dev-spi.o
obj-$(CONFIG_AR79_DEV_USB)		+= dev-usb.o
obj-$(CONFIG_AR79_DEV_WMAC)		+= dev-wmac.o

#
# Miscellaneous objects
#
obj-$(CONFIG_AR79_NVRAM)		+= nvram.o
obj-$(CONFIG_AR79_PCI_ATH9K_FIXUP)	+= pci-ath9k-fixup.o
obj-$(CONFIG_AR79_ROUTERBOOT)		+= routerboot.o

#
# Machines
#

obj-$(CONFIG_AR79_MACH_WAFI_R1001)	+= mach-wafi-r1001.o
