all:httpsend
ALL_OBJS = httpsend_main.o provider.o oray.o md5.o base64.o blowfish.o

OUTFILE=$(OUTDIR)/$(TARGET_ARCH)/yddns
httpsend: $(ALL_OBJS)
# $(CC) -o yddns $(ALL_OBJS) $(LIBS) 
	$(CC) -o getinternet $(ALL_OBJS)  
	$(STRIP) httpsend
httpsend_main.o: httpsend_main.c 
	$(CC) $(INCLUDE) $(FLAGS)  -O2 -c httpsend_main.c
provider.o:   provider.c
	$(CC) $(INCLUDE) $(FLAGS)  -O2 -c provider.c
oray.o:   oray.c
	$(CC) $(INCLUDE) $(FLAGS)  -O2 -c oray.c
md5.o:	md5.c
	$(CC) $(INCLUDE) $(FLAGS)  -O2 -c md5.c
base64.o:	base64.c
	$(CC) $(INCLUDE) $(FLAGS)  -O2 -c base64.c
blowfish.o:	blowfish.c
	$(CC) $(INCLUDE) $(FLAGS)  -O2 -c blowfish.c
    
clean:  
	rm -f httpsend *.o


